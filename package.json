{
  "name": "indeps",
  "version": "0.0.1",
  "description": "üîç Visualize the dependencies & sub-dependencies in your Javascript application.",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "engines": {
    "node": "~16.10.0"
  },
  "scripts": {
    "build:node": "PKG_ENV=prod rollup -c",
    "build:client": "webpack --mode production",
    "dev:node": "PKG_ENV=dev rollup -c",
    "dev:node:watch": "yarn dev:node --watch",
    "dev:client": "webpack --mode development",
    "dev:client:watch": "yarn dev:client --watch",
    "start": "node lib/cli.js",
    "start:watch": "nodemon lib/cli.js",
    "debug": "node --inspect-brk lib/cli.js",
    "debug:watch": "nodemon --inspect-brk lib/cli.js",
    "test": "jest --coverage --verbose",
    "test:watch": "npm test -- --watch --verbose"
  },
  "keywords": [
    "dependency",
    "visualizer",
    "javascript",
    "react",
    "webpack",
    "lock",
    "package"
  ],
  "author": "Ben Kincaid",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sirv": "^1.0.18",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.4",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/yargs": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.0",
    "nodemon": "^2.0.13",
    "rollup": "^2.60.0",
    "ts-jest": "^27.0.7",
    "tsc": "^2.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "packageManager": "yarn@3.1.0"
}
